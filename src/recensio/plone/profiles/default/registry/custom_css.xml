<?xml version="1.0" encoding="utf-8"?>
<registry>
  <records interface="plone.app.theming.interfaces.IThemeSettings"
           prefix="plone.app.theming.interfaces.IThemeSettings"
  >
    <value key="absolutePrefix">/++theme++barceloneta</value>
    <value key="currentTheme">barceloneta</value>
    <value key="custom_css">:root {
  --bs-primary: #e8e1d0;
  --bs-link-color: #7f4b2b;
  --plone-toolbar-text-color: var(--bs-dark);
}
.navbar-barceloneta {
  --bs-navbar-background: #e8e1d0;
  --bs-navbar-color: #7f4b2b;
}
.btn-outline-light {
  --bs-btn-color: #7f4b2b;
}
#edit-zone .dropdown-item, #edit-zone .nav-link {
  color: var(--bs-white);
}</value>
  </records>
</registry>
